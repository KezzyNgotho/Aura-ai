    "deploy:hardhat:base": "hardhat run scripts/hardhat-deploy.js --network base",
    "deploy:hardhat:optimism": "hardhat run scripts/hardhat-deploy.js --network optimism",
    "deploy:hardhat:arbitrum": "hardhat run scripts/hardhat-deploy.js --network arbitrum",
    "deploy:hardhat:polygon": "hardhat run scripts/hardhat-deploy.js --network polygon",
    "contracts:info": "ts-node scripts/deploy.ts",
    "contracts:extract-abis": "ts-node scripts/extract-abis.ts",
    "build": "tsc --noEmit && wrangler deploy --dry-run",
    "dev": "concurrently \"npx @modelcontextprotocol/inspector\" \"wrangler dev\" --kill-others",
    "start": "wrangler dev",
    "test": "vitest run",
    "cf-typegen": "wrangler types",
    "dev:nullshot": "nullshot dev",
    "postinstall": "nullshot install"
  },
