    "concurrently": "^9.1.2",
    "dotenv": "^17.2.3",
    "ethers": "^5.8.0",
    "hardhat": "^2.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "~3.2.0",
    "wrangler": "^4.32.0"
  },
  "dependencies": {
