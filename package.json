{
  "name": "aura-ai",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "deploy": "wrangler deploy",
    "deploy:contracts": "node scripts/deploy-contracts.js",
    "deploy:contracts:testnet": "node scripts/deploy-contracts.js",
    "deploy:contracts:base": "node scripts/deploy-contracts.js",
    "deploy:contracts:optimism": "node scripts/deploy-contracts.js",
    "deploy:contracts:arbitrum": "node scripts/deploy-contracts.js",
    "deploy:contracts:polygon": "node scripts/deploy-contracts.js",
